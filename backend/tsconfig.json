{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "outDir": "dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "strict": true,
    "paths": {
      "@prisma/*": ["../prisma/*"]
    }
  },
  "include": [
    "src/**/*",
    "../prisma/**/*" 
  ],
  "exclude": [
    "node_modules"
  ]
}
