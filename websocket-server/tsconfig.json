{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "outDir": "./dist",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@prisma/*": ["../prisma/*"]
    }
  },
  "include": [
    "src/**/*",       
    "../prisma/**/*"   
  ],
  "exclude": [
    "node_modules"
  ]
}
