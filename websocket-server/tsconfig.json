{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "rootDir": "./src",
    "outDir": "./dist",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@prisma/*": ["../prisma/*"]
    },
    "moduleResolution": "node"
  },
  "include": [
    "src/**/*"   
  ],
  "exclude": [
    "node_modules"
  ]
}
